{
  "version": 2,
  "name": "native-server",
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/public/(.*)", "dest": "/public/$1", "headers": { "cache-control": "public, max-age=31536000, immutable" } },
    { "src": "/views/(.*)", "dest": "/views/$1" },
    { "src": "/(.*)", "dest": "/server.js" }
  ]
}
