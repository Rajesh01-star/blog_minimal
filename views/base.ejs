    <%- include("partials/individual") -%>
  <% } else if (titleSite === 'imgGallery') { %>
    <%- include("partials/imgGallery") -%>
  <% } else if (titleSite === 'admin') { %>
    <%- include("partials/admin") -%>
  <% } %>
</main>
<%- include("partials/footer") -%>
