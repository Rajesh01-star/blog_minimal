  <% if (titleSite === 'home') { %>
    <%- include("partials/home") -%>  
  <% } else if (titleSite === 'about') { %>
    <%- include("partials/about") -%>  
  <% } else if (titleSite === 'blog') { %>
    <%- include("partials/blog") -%>  
  <% } else if (titleSite === 'contact') { %>
    <%- include("partials/contact") -%>  
